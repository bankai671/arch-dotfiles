{
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml"
  },
  "editor.inlayHints.enabled": "off",
  "window.titleBarStyle": "native",
  "window.customMenuBarAltFocus": false,
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'FiraCode Nerd Font'",
  "editor.mouseWheelZoom": true,
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#D0758B",
    "activityBarBadge.background": "#D0758B",
    "badge.background": "#D0758B",
    "button.background": "#D0758B",
    "button.hoverBackground": "#D0758Be6",
    "editorGutter.modifiedBackground": "#D0758B",
    "focusBorder": "#D0758B",
    "panelTitle.activeBorder": "#D0758B",
    "progressBar.background": "#D0758B",
    "statusBar.debuggingBackground": "#D0758B",
    "statusBarItem.focusBorder": "#D0758B",
    "statusBarItem.remoteBackground": "#D0758B",
    "tab.activeBorderTop": "#D0758B",
    "terminal.tab.activeBorder": "#D0758B",
    "welcomePage.progress.foreground": "#D0758B",
    "textLink.foreground": "#D0758B",
    "textLink.activeForeground": "#D0758c95",
    "inputOption.activeBackground": "#D0758B82",
    "inputOption.activeBorder": "#D0758B",
    "editorSuggestWidget.highlightForeground": "#D0758B",
    "editorSuggestWidget.focusHighlightForeground": "#D0758B",
  },
  "http.proxySupport": "off",
  "workbench.list.smoothScrolling": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.printWidth": 90,
  "code-runner.runInTerminal": true,
  "window.menuBarVisibility": "hidden",
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "C_Cpp.default.compilerPath": "/usr/bin/gcc",
  "errorLens.excludeBySource": [
    "ts(2307)"
  ],
}
